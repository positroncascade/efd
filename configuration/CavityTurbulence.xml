<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <flow Re="10000" />
    <simulation finalTime="10" >turbulence</simulation>
    <timestep dt="1" tau="0.5" />
    <solver gamma="0.5" />
    <geometry dx="0.1" dy="0.1" dz="0.1" sizeX="20" sizeY="20" sizeZ="20" />
    <environment gx="0" gy="0" gz="0" />
    <walls periodicX="false" periodicY="false" periodicZ="false">
        <left>
            <vector x="0" y="0" z="0" />
        </left>
        <right>
            <vector x="0" y="0" z="0" />
        </right>
        <top>
            <vector x="1" y="0" z="0" />
        </top>
        <bottom>
            <vector x="0" y="0" z="0" />
        </bottom>
        <front>
            <vector x="0" y="0" z="0" />
        </front>
        <back>
            <vector x="0" y="0" z="0" />
        </back>
    </walls>
    <vtk interval="1">somefile</vtk>
    <stdOut interval="10" />
    <parallel numProcessorsX="1" numProcessorsY="1" numProcessorsZ="1" />
</configuration>
